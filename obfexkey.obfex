(async function() {
    // Base32 √á√∂z√ºc√º Fonksiyon
    const b32d = (s) => {
        const a = "ABCDEFGHIJKLMNOPQRSTUVWXYZ234567";
        let p = [], q = 0, l = 0;
        s = s.replace(/=+$/, "");
        for (let i = 0; i < s.length; i++) {
            let v = a.indexOf(s[i].toUpperCase());
            if (v === -1) continue;
            q = (q << 5) | v; l += 5;
            if (l >= 8) { p.push(String.fromCharCode((q >> (l - 8)) & 255)); l -= 8; }
        }
        return p.join('');
    };

    try {
        // 1. ≈ûifreli veriyi i√ßeren dosyayƒ± √ßek
        const res = await fetch("obfexfixingsearch.txt");
        if(!res.ok) throw 0;
        const encryptedData = (await res.text()).trim();

        // 2. Kƒ±rma ƒ∞≈ülemi: Base64 -> Reverse -> Base32
        const s1 = atob(encryptedData);
        const s2 = s1.split("").reverse().join("");
        const s3 = b32d(s2);
        const finalHtml = decodeURIComponent(escape(s3));

        // 3. √á√∂z√ºlen i√ßeriƒüi yazdƒ±r
        document.open();
        document.write(finalHtml);
        
        // Modern Beyaz Rozet
        const b = document.createElement("div");
        b.style = "position:fixed;bottom:12px;right:12px;background:rgba(255,255,255,0.08);color:#fff;padding:4px 10px;border:1px solid rgba(255,255,255,0.2);border-radius:50px;font-size:9px;z-index:999999;font-weight:500;letter-spacing:0.8px;backdrop-filter:blur(10px);pointer-events:none;font-family:sans-serif;box-shadow:0 4px 15px rgba(0,0,0,0.5);text-transform:uppercase;";
        b.innerHTML = "üõ°Ô∏è OBFEX SECURE";
        document.body.appendChild(b);
        document.close();
    } catch(e) {
        document.body.innerHTML = "Veri dosyasƒ± (obfexfixingsearch.txt) okunamadƒ±!";
    }
})();
